   "source": [
    "# TODO: calculate the infection probability for each category\n",
    "wealth_infection_prob = HIV_data.groupby('Wealth category')['Infected'].mean()\n",
    "# TODO: calculate the infection probability by age group\n",
    "age_bins = [0, 18, 35, 50, 100]  \n",
    "age_labels = ['<18', '18-35', '35-50', '50+']\n",
    "HIV_data['Age_Group'] = pd.cut(HIV_data['Age'],bins=age_bins, labels=age_labels)\n",
    "\n",
    "age_infection_prob = HIV_data.groupby('Age_Group',observed=True)['Infected'].mean()\n",
    "# TODO: calculate the infection probability by gender\n",
    "gender_infection_prob = HIV_data.groupby('Gender')['Infected'].mean()\n",
    "# TODO: print the infection tables\n",
    "print(\"Infection Probability by wealth category\")\n",
    "print(tabulate(wealth_infection_prob.reset_index(), \n",
    "               headers=['Wealth Category', 'Infection Probability'],\n",
    "               tablefmt='fancy_grid'))\n",
    "print(\"Infection probability by Age group \")\n",
    "print(tabulate(age_infection_prob.reset_index(),\n",
    "               headers=['Age group','Infection Probability'],\n",
    "               tablefmt='fancy_grid'))\n",
    "print(\"Infection Probability by Gender\")\n",
    "print(tabulate(gender_infection_prob.reset_index(),\n",
    "               headers=['Gender group','Infection Probability'],\n",
    "               tablefmt='fancy_grid'))"
   ]
