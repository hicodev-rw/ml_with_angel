    "# TODO: Predict the infection using the logistic regression model\n",
    "y_pred = model.predict(X_test)\n",
    "# TODO:  Calculate the confusion matrix\n",
    "conf_matrix = confusion_matrix(y_test, y_pred)\n",
    "print('Confusion Matrix',conf_matrix)\n",
    "# TODO:  visualize the confusion matrix\n",
    "plt.figure(figsize=(6,4))\n",
    "sns.heatmap(conf_matrix, annot=True, fmt='d', cmap='Blues',xticklabels=['Not Infected', 'Infected'], yticklabels=['Not Infected', 'Infected'])\n",
    "plt.xlabel('Predicted')\n",
    "plt.ylabel('Actual')\n",
    "plt.title('Confusion Matrix')\n",
    "plt.show()\n",
    "# TODO:  Calculate the accuracy using the accuracy_score function\n",
    "accuracy = accuracy_score(y_test, y_pred)\n",
    "print('Accuracy using accuracy score is', accuracy)\n",
    "# TODO:  Calculate the accuracy based on the confusion matrix\n",
    "true_positives = conf_matrix[1, 1]\n",
    "true_negatives = conf_matrix[0, 0 ]\n",
    "total_samples = conf_matrix.sum()\n",
    "manual_accuracy = (true_positives + true_negatives) / total_samples\n",
    "print(\"Accuracy based on confusion matrix\", manual_accuracy)"
